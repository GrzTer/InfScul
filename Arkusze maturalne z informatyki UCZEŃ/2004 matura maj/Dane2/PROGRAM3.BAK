program program2;
var
tablica: array[1..20] of real;
tmp,wynik:real;
i,j:integer;
p,dane:text;
begin
for j:=1 to 20 do tablica[j] := 0;
assign(dane,'oporniki.txt');
reset
   for i:=1 to 20 do
     for j:=1 to i do
     begin
       read(dane,tmp);
       tablica[i]:=tablica[i]+tmp;
     end;
   wynik := 1/r*tmp;
   wynik:=1/wynik;
   assign(p,'wynik2.txt');
   rewrite(p);
   write(p,wynik:8:2);
   close(p);
end.